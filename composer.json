{
  "name": "brieucthomas/ergast-api-client",
  "type": "library",
  "description": "PHP client for Ergast Developer API",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Ergast\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergast\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1",
    "psr/http-message": "^1.0",
    "psr/cache": "^1.0",
    "php-http/httplug": "^1.1",
    "php-http/discovery": "^1.0",
    "php-http/client-implementation": "^1.0",
    "php-http/client-common": "^1.3",
    "php-http/cache-plugin": "^1.4",
    "pagerfanta/pagerfanta": "^1.0",
    "jms/serializer": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "friendsofphp/php-cs-fixer": "^2.0",
    "php-http/guzzle6-adapter": "^1.0",
    "php-http/mock-client": "^1.0",
    "guzzlehttp/psr7": "^1.2",
    "cache/array-adapter": "^0.4"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
